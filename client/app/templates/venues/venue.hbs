<div class="col-sm-8 col-sm-offset-4 col-md-9 col-md-offset-3 main">
  <div class="row">
    <div class="col-sm-6 col-md-6">
      <h3>{{model.name}}</h3>
      {{#each model.locations as |location|}}
        <p>{{location.address}}</p>
      {{/each}}
    </div>
    <div class="col-sm-6 col-md-6">
      {{pw-venue-map class="venue__map" locations=model.locations}}
    </div>
  </div>
</div>

